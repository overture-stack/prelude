{
	"name": "Demo Dictionary",
	"version": "2.0",
	"description": "All data is intended for demo purposes only and does not represent any real or operational information.",
	"meta": {
		"author": "Overture Team",
		"created": "2025-10-06",
		"contact": "data-team@clinical.org"
	},
	"schemas": [
		{
			"name": "Demo Schema",
			"description": "Flat-file schema combining participant, diagnosis, treatment, and sample data",
			"fields": [
				{
					"name": "submitter_participant_id",
					"displayName": "Participant ID",
					"valueType": "string",
					"description": "Unique identifier of the participant within the study",
					"restrictions": {
						"required": true,
						"regex": "^[A-Za-z0-9\\-\\._]{1,64}$"
					},
					"unique": true,
					"meta": { "examples": ["PAT-000001", "PAT-000002"] }
				},
				{
					"name": "sex",
					"valueType": "string",
					"description": "Biological sex of the participant",
					"restrictions": {
						"required": true,
						"codeList": ["male", "female", "other", "unknown"]
					},
					"meta": { "examples": ["male", "female"] }
				},
				{
					"name": "date_of_birth",
					"valueType": "string",
					"description": "Participant's date of birth in YYYY-MM-DD format",
					"restrictions": {
						"required": true,
						"regex": "^\\d{4}-\\d{2}-\\d{2}$"
					},
					"meta": { "examples": ["1980-01-01", "1992-07-15"] }
				},
				{
					"name": "vital_status",
					"valueType": "string",
					"description": "Current vital status of the participant",
					"restrictions": {
						"required": true,
						"codeList": ["alive", "deceased", "unknown"]
					},
					"meta": { "examples": ["alive", "deceased"] }
				},
				{
					"name": "date_of_death",
					"valueType": "string",
					"description": "Date of death if participant is deceased; otherwise empty",
					"restrictions": {
						"if": {
							"conditions": [{ "fields": ["vital_status"], "match": { "value": "deceased" } }]
						},
						"then": { "required": true, "regex": "^\\d{4}-\\d{2}-\\d{2}$" },
						"else": { "empty": true }
					},
					"meta": { "examples": ["2025-01-01"] }
				},
				{
					"name": "submitter_diagnosis_id",
					"valueType": "string",
					"description": "Unique identifier for each diagnosis record",
					"restrictions": {
						"required": true,
						"regex": "^[A-Za-z0-9\\-\\._]{1,64}$"
					},
					"unique": true,
					"meta": { "examples": ["DIAG-000001", "DIAG-000002"] }
				},
				{
					"name": "diagnosis_date",
					"valueType": "string",
					"description": "Date the diagnosis was recorded (YYYY-MM-DD)",
					"restrictions": {
						"required": true,
						"regex": "^\\d{4}-\\d{2}-\\d{2}$"
					},
					"meta": { "examples": ["2020-05-12", "2021-03-22"] }
				},
				{
					"name": "disease_stage",
					"valueType": "string",
					"description": "Stage of disease at diagnosis",
					"restrictions": {
						"codeList": ["Stage I", "Stage II", "Stage III", "Stage IV"]
					},
					"meta": { "examples": ["Stage II", "Stage IV"] }
				},
				{
					"name": "submitter_treatment_id",
					"valueType": "string",
					"description": "Unique identifier for each treatment record",
					"restrictions": {
						"required": true,
						"regex": "^[A-Za-z0-9\\-\\._]{1,64}$"
					},
					"unique": true,
					"meta": { "examples": ["TRT-000001", "TRT-000002"] }
				},
				{
					"name": "treatment_type",
					"valueType": "string",
					"description": "Type of treatment administered",
					"restrictions": {
						"codeList": ["Chemotherapy", "Immunotherapy", "Radiation", "Surgery", "Targeted Therapy"]
					},
					"meta": { "examples": ["Chemotherapy", "Immunotherapy"] }
				},
				{
					"name": "start_date",
					"valueType": "string",
					"description": "Start date of the treatment (YYYY-MM-DD)",
					"restrictions": {
						"required": true,
						"regex": "^\\d{4}-\\d{2}-\\d{2}$"
					},
					"meta": { "examples": ["2020-06-01"] }
				},
				{
					"name": "end_date",
					"valueType": "string",
					"description": "End date of the treatment (YYYY-MM-DD)",
					"restrictions": {
						"if": { "conditions": [{ "fields": ["start_date"], "match": { "exists": true } }] },
						"then": { "regex": "^\\d{4}-\\d{2}-\\d{2}$" }
					},
					"meta": { "examples": ["2020-09-01"] }
				},
				{
					"name": "response",
					"valueType": "string",
					"description": "Response to the treatment",
					"restrictions": {
						"codeList": ["Complete Response", "Partial Response", "Stable Disease", "Progressive Disease"]
					},
					"meta": { "examples": ["Complete Response", "Partial Response"] }
				},
				{
					"name": "submitter_sample_id",
					"valueType": "string",
					"description": "Unique identifier for each sample collected",
					"restrictions": {
						"required": true,
						"regex": "^[A-Za-z0-9\\-\\._]{1,64}$"
					},
					"unique": true,
					"meta": { "examples": ["SMP-000001", "SMP-000002"] }
				},
				{
					"name": "tissue_type",
					"valueType": "string",
					"description": "Type of tissue collected for the sample",
					"restrictions": {
						"required": true,
						"codeList": ["Blood", "Tumor", "Normal Tissue", "Saliva"]
					},
					"meta": { "examples": ["Blood", "Tumor"] }
				},
				{
					"name": "collection_date",
					"valueType": "string",
					"description": "Date the sample was collected (YYYY-MM-DD)",
					"restrictions": {
						"regex": "^\\d{4}-\\d{2}-\\d{2}$"
					},
					"meta": { "examples": ["2020-06-05"] }
				}
			],
			"restrictions": {
				"uniqueKey": ["submitter_participant_id"]
			}
		}
	]
}
