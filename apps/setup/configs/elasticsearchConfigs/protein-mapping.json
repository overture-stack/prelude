{
  "index_patterns": [
    "protein-*"
  ],
  "aliases": {
    "protein_centric": {}
  },
  "mappings": {
    "properties": {
      "hugo_symbol_autocomplete": {
        "type": "keyword",
        "fields": {
          "analyzed": {
            "type": "text",
            "analyzer": "autocomplete_analyzed",
            "search_analyzer": "lowercase_keyword"
          },
          "lowercase": {
            "type": "text",
            "analyzer": "lowercase_keyword"
          },
          "prefix": {
            "type": "text",
            "analyzer": "autocomplete_prefix",
            "search_analyzer": "lowercase_keyword"
          }
        }
      },
      "data": {
        "type": "object",
        "properties": {
          "hugo_symbol_a": {
            "type": "keyword",
            "copy_to": ["hugo_symbol_autocomplete"]
          },
          "hugo_symbol_b": {
            "type": "keyword",
            "copy_to": ["hugo_symbol_autocomplete"]
          },
          "neighborhood": {
            "type": "integer"
          },
          "neighborhood_transferred": {
            "type": "integer"
          },
          "fusion": {
            "type": "integer"
          },
          "cooccurence": {
            "type": "integer"
          },
          "homology": {
            "type": "integer"
          },
          "coexpression": {
            "type": "integer"
          },
          "coexpression_transferred": {
            "type": "integer"
          },
          "experiments": {
            "type": "integer"
          },
          "experiments_transferred": {
            "type": "integer"
          },
          "database": {
            "type": "integer"
          },
          "database_transferred": {
            "type": "integer"
          },
          "textmining": {
            "type": "integer"
          },
          "textmining_transferred": {
            "type": "integer"
          },
          "combined_score": {
            "type": "integer"
          },
          "dataset_name": {
            "type": "keyword"
          },
          "is_oncogene_a": {
            "type": "keyword",
            "null_value": "No Data"
          },
          "is_tumor_suppressor_gene_a": {
            "type": "keyword",
            "null_value": "No Data"
          },
          "is_oncogene_b": {
            "type": "keyword",
            "null_value": "No Data"
          },
          "is_tumor_suppressor_gene_b": {
            "type": "keyword",
            "null_value": "No Data"
          },
          "is_wdr_a": {
            "type": "keyword"
          },
          "is_wdr_b": {
            "type": "keyword"
          }
        }
      },
      "submission_metadata": {
        "type": "object",
        "properties": {
          "submission_id": {
            "type": "keyword",
            "null_value": "No Data"
          },
          "source_file_hash": {
            "type": "keyword",
            "null_value": "No Data"
          },
          "processed_at": {
            "type": "date"
          }
        }
      }
    }
  },
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 0,
    "index": {
      "max_result_window": "10000",
      "max_terms_count": "300000"
    },
    "analysis": {
      "analyzer": {
        "autocomplete_analyzed": {
          "tokenizer": "standard",
          "filter": ["lowercase", "edge_ngram"]
        },
        "autocomplete_prefix": {
          "tokenizer": "keyword",
          "filter": ["lowercase", "edge_ngram"]
        },
        "lowercase_keyword": {
          "tokenizer": "keyword",
          "filter": ["lowercase"]
        }
      },
      "filter": {
        "edge_ngram": {
          "type": "edge_ngram",
          "min_gram": "1",
          "max_gram": "20",
          "side": "front"
        }
      }
    }
  }
}